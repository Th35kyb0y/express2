@charset "utf-8";
/* CSS Document */

body {
    font-family: Roboto, Arial, sans-serif !important;
}

.bg_img {
     background-image: url("../../../../assets/images/homes/home_bg.png");
    background-repeat: round;
    padding: 30px 50px 30px 96px;
    height: 600px;
}

.bg_white {
    background: rgba(255, 255, 255, 0.8);
    padding: 30px;
    height: 380px;
    width: 1140px;
    text-align: justify;
}
.heading_f{     background-color: #c6000c;
    color: #fff;}
.carousel-item-next,
.carousel-item-prev,
.carousel-item.active {
    display: block;
    height: 400px;
}

.carousel-caption p {
    font-size: 25px;
}

.carousel-caption {
    position: absolute;
    right: 15%;
    bottom: 6.25rem;
    left: 15%;
    padding-top: 1.25rem;
    padding-bottom: 1.25rem;
    color: #fff;
    text-align: center;
}

.floot {
    float: left !important;
}

.ml-3 {
    margin-left: 30px;
}

.ml-2 {
    margin-left: 20px;
}

.box_2 {
    background-color: #eaebec;
    height: 500px;
    padding: 50px 110px 50px 110px;
}

.carousel-inner {
    background-color: #1e415a !important;
}

.carousel {
    margin-bottom: 0px !important;
}

.box_4 {
    background-color: #ebecee;
    height: 500px;
    padding: 50px;
}


.box_5 {
    background-color: #fff;
    height: 500px;
    padding: 50px 110px 50px 110px;
}

.box_6 {
    background-color: #fff;
    min-height: 500px;
    padding: 25px 10px 10px 10px;
}

.bg_white2 {
    background: #fff;
    float: left;
}


.BG_TOP {
    width: 600px;
    margin-left: 280px;
    height: 48px;
    margin-bottom: 30px;
    text-align: center;
    color: #000;
    background: rgba(255, 255, 255, 10);
}


.bg_white3 {
    background: #fff;
    padding: 30px 80px 30px 80px;
    height: 500px;
    width: 1140px;
    text-align: justify;
}

.bg_blue {
    background-color: #1e415a;
    text-align: center;
    color: #fff;
    font-weight: 500;
}

.cat_border {
    border: 1px solid #d3d3d3;
    margin: 0px 5px 10px 5px;
}

.rating {
    color: #fff;
    background: #388e3c;
    width: auto;
    height: 19px;
    text-align: center;
    border-radius: 5px;
    line-height: normal;
    font-size: 12px;
    display: inline-flex;
    align-items: center;
    padding: 10px 13px;
    margin-right: 10px;

}

p {
    margin-bottom: 2px;
}

hr {
    margin: 4px;
    color: #d3d3d3;
}

.price_p {
    font-weight: 500;
    color: #000;
}

.og_price {
    text-decoration: line-through;
    font-size: 14px;
    color: #878787;
    margin-left: 10px;
}

.discount_per {
    color: #388e3c;
    font-size: 13px;
    letter-spacing: -.2px;
    font-weight: 500;
    margin-left: 10px;
}

// .plus .input-group{ width: 50% !important;}
.input-group-text {
    font-size: 16px !important;
    font-weight: 500 !important;
}
