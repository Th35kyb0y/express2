.cs-div-border{
    border: 1px solid #363434;
}

.topnav {
    overflow: hidden;
    background-color: #333;
  }
  
  .topnav a {
    float: left;
    color: #f2f2f2;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
    font-size: 17px;
  }
  
  .topnav a:hover {
    background-color: #ddd;
    color: black;
  }
  
  .topnav a.active {
    background-color: #c6000c;
    color: white;
  }
  .box-min-w{
    min-width: 96px;
    min-height: 96px;
  }
  