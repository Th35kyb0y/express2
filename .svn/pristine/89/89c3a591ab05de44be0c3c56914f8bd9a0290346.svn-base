.modal-content {
    position: relative;
    display: flex;
    flex-direction: column;
    width: 90% !important;
    pointer-events: auto;
    background-color: #fff;
    background-clip: padding-box;
    border: 1px solid rgba(0, 0, 0, 0.2);
    border-radius: 0.3rem;
    outline: 0;
}

.bg_gr{ background-color: #293747; margin-top: 73px;}

th{
    // white-space: nowrap;
    min-width: 150px;
    color: #fff;
    background-color: #293747;
}

.table-fixed-header thead {
    position: sticky;
    top: 0;
    background-color: #fff; /* Adjust as needed */
    z-index: 999;
}

.table-fixed-last-column th:last-child,
.table-fixed-last-column td:last-child {
    position: sticky;
    right: 0;
    background-color: #c9242c; /* Adjust as needed */
    float: right;
}

.loader {
    width: 48px;
    height: 48px;
    border: 5px solid #cdb0b0;
    border-bottom-color: #FF3D00;
    border-radius: 50%;
    display: inline-block;
    box-sizing: border-box;
    animation: rotation 1s linear infinite;
    }

    @keyframes rotation {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(360deg);
    }
    } 