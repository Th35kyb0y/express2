<!-- main start here-->
<main id="main">
  <section class="inner-page p-0 sec_body">
      <div class="container-fluid ">
          <div class="row">
              <div class="col-12 middle">
                  <!--sidebarTop start here-->
                  <app-sidebar></app-sidebar>
                  <!--sidebarTop end here-->
                  <!-- ======= Breadcrumbs ======= -->
                  <section class="breadcrumbs bredcrumb_top">
                      <div class="container-fluid ">
                          <div class="d-flex justify-content-between align-items-center">
                              <ol class="breadcrumb  card_partd">
                                  <li class="breadcrumb-item active"><a href="#">My Account</a></li>
                                  <li class="breadcrumb-item"> <a routerLink="#" routerLinkActive="active"
                                          style="color: #c6000c">My Offers</a></li>
                              </ol>
                          </div>
                      </div>
                  </section>
                  <!-- End Breadcrumbs -->

                  <div class="row mt-2">
                      <div class="col-8 s0">My Offers</div>
                  </div>
                  <div class="row">
                    
                          <hr>

                          <ng-container *ngFor="let item of coupons">
                            <div class="col-12 pull-left" *ngIf="item.imagePath">
                                <h1 class="offer"><a ><img [src]="imtUrl+''+item.imagePath" class="img-fluid" alt="" /></a></h1>

                            </div>
                            <div class="col-md-6 pull-left" *ngIf="!item.imagePath">

                                <div class="card" style="width: 18rem;">
                                    <div class="card-header">
                                        <strong>Coupon Code: {{item.couponCode}}</strong>
                                    </div>
                                    <ul class="list-group list-group-flush">
                                      <li class="list-group-item">
                                            <strong>Avail {{item.discount}} 
                                            <ng-container *ngIf="item.isInUsePercent">%</ng-container>
                                            <ng-container *ngIf="!item.isInUsePercent">â‚¹</ng-container>
                                            OFF ON YOUR CART VALUE
                                        </strong>
                                        </li>
                                      <li class="list-group-item">  VALID UNTIL {{item.expirationDate | date:'dd MMM YYYY'}} </li>
                                      <li class="list-group-item">{{item.message}} </li>
                                      <li class="list-group-item">{{item.description}} </li>
                                    </ul>
                                  </div>


    
                            </div>
                          </ng-container>
                       

                          <!-- <div class="col-12">

                            <div class="col-12 pull-left">
                                <h1 class="offer"><a routerLink="/"><img src="http://cfx.ceasefire.biz/assets/voucher/Festive_offer.png" width="1000" height="400" alt="" /></a></h1>

                            </div>
                            <div class="col-12 pull-left">

                                <h1 class="offer"><a routerLink="/"><img src="http://cfx.ceasefire.biz/assets/voucher/privilege_member.png" width="1000" height="400" alt="" /></a></h1>
                            </div>
                            <div class="col-12 pull-left">

                                <h1 class="offer"><a routerLink="/"><img src="http://cfx.ceasefire.biz/assets/voucher/Referral_discount.png" width="1000" height="400" alt="" /></a></h1>
                            </div>
                      </div> -->
                  </div>



              </div>
          </div>

      </div>
  </section>
</main>
<!-- End #main -->
