hr{ margin: 1px 0px 12px 0px;}

.popup-container {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 9999;
  }
  
  .popup-content5 {
    background-color: #fff;
    /* padding: 20px; */
    /* border-radius: 4px; */
    width: 30px;
  }
  .slc{
    width: 25% !important;
    margin-left: 5% !important;
  }
  .popup-content-int {
    background-color: #fff;
    width: 800px;
    height: 60vh;
    overflow-x: auto;
  
  }
  
  .inter {
    margin-left: 4%;
    margin-right: 4%;
    margin-top: 2%;
  }
  
  .popup-content-int h3 {
    margin-top: 2%;
    margin-left: 2%;
  }
  .cf{
    display: flex;
    flex-wrap: wrap;
  }
  .cf div{
    width: 45% !important;
    margin-top: 5% !important;
  }
  .container2{
    display: flex;
    justify-content: space-between;
  }
  .container2 div{
    margin-left: 2%;
    margin-right: 2%;
  }

  /* your.component.css */
.card {
    margin-top: 150px;
  }
  
  .card-header {
    background-color: #f8f9fa; /* Set your desired background color */
  }
  
  .nav-tabs {
    border-bottom: 1px solid #dee2e6; /* Set your desired border color */
  }
  
  .nav-link {
    color: #495057; /* Set your desired text color */
  }
  
  .nav-link.active {
    background-color: #007bff; /* Set your desired active tab background color */
    color: #ffffff; /* Set your desired active tab text color */
  }
  