  ::ng-deep .tooltip {

    --bs-tooltip-bg:white;
    --bs-tooltip-color:black;
    z-index: 9999999;
  }

  .node {
    display: inline-block;
    margin-right: 10px;
    cursor: pointer;
    position: relative;
  }

  .children {
    position: absolute;
    top: 0;
    right: 100%;
    background-color: #fff;
    border: 1px solid #ccc;
    padding: 5px;
    z-index: 1;
  }

  .tree-dropdown {
    position: relative;
  }

  .tree-dropdown-select {
    width: 100%;
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 4px;
    background-color: #fff;
    font-size: 14px;
    color: #333;
  }

  /* Style the dropdown arrow */
  .tree-dropdown-select:after {
    content: '\25BE'; /* Unicode character for down arrow */
    position: absolute;
    top: 50%;
    right: 8px;
    transform: translateY(-50%);
  }

  /* Style the optgroups */
  optgroup {
    font-weight: bold;
  }


  .toggle-switch {
    position: relative;
    display: inline-block;
    width: 50px;
    height: 24px;
    background-color: #ccc;
    border-radius: 12px;
    cursor: pointer;
  }

  .toggle-slider {
    position: absolute;
    top: 2px;
    left: 2px;
    width: 20px;
    height: 20px;
    background-color: white;
    border-radius: 50%;
    transition: 0.4s;
  }

  .toggle-switch.active {
    background-color: #66bb6a; /* Color when toggle is active */
  }

  .toggle-switch.active .toggle-slider {
    transform: translateX(26px);
  }





.ng-select ::ng-deep .ng-dropdown-panel .ng-dropdown-panel-items .ng-optgroup {
  white-space: nowrap !important;
  overflow: hidden !important;
  text-overflow: ellipsis !important;
  user-select: none !important;
  padding: 8px 10px !important;
  font-weight: 500 !important;
  color: rgba(0, 0, 0, 0.54) !important;
}

.ng-select ::ng-deep .ng-dropdown-panel .ng-dropdown-panel-items .ng-option {
  background-color: #fff;
  color: rgba(0, 0, 0, 0.87);
  padding: 8px 10px;
}

.ng-select ::ng-deep .ng-dropdown-panel .ng-dropdown-panel-items .ng-option.ng-option-child {
  padding-left: 22px;
}

.ng-select ::ng-deep .ng-dropdown-panel.ng-select-bottom {
  top: 100%;
  border-bottom-right-radius: 4px;
  border-bottom-left-radius: 4px;
  border-top-color: #e6e6e6;

}

.ng-select ::ng-deep .ng-dropdown-panel {
  box-sizing: border-box;
  position: absolute;
  opacity: 0;
  width: 100%;
  z-index: 1050;
  margin-top: 5px;
  margin-left: -12px;
}

.tooltip ::ng-deep {
  --bs-tooltip-zindex: 1080;
  --bs-tooltip-max-width: 100% !important;
  --bs-tooltip-padding-x: 0.5rem;
  --bs-tooltip-padding-y: 0.25rem;
  --bs-tooltip-margin: ;
  --bs-tooltip-font-size: 0.875rem;
  --bs-tooltip-color: var(--bs-body-bg);
  --bs-tooltip-bg: var(--bs-emphasis-color);
  --bs-tooltip-border-radius: var(--bs-border-radius);
  --bs-tooltip-opacity: 0.9;
  --bs-tooltip-arrow-width: 0.8rem;
  --bs-tooltip-arrow-height: 0.4rem;
  z-index: var(--bs-tooltip-zindex);
  display: block;
  margin: var(--bs-tooltip-margin);
  font-family: var(--bs-font-sans-serif);
  font-style: normal;
  font-weight: 400;
  line-height: 1.5;
  text-align: left;
  text-align: start;
  text-decoration: none;
  text-shadow: none;
  text-transform: none;
  letter-spacing: normal;
  word-break: normal;
  white-space: normal;
  word-spacing: normal;
  line-break: auto;
  font-size: var(--bs-tooltip-font-size);
  word-wrap: break-word;
  opacity: 0;
}

