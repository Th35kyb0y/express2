<!-- tabs.component.html -->
<div class="modal-header">
    <h5 class="modal-title"> Follow up Details</h5>
    <button type="button" class="btn-close" aria-label="Close" (click)="closeModal('Dismissed')">
      <!-- <span aria-hidden="true">&times;</span> -->
    </button>
   
  </div>
  <div class="modal-body" style="  height: 80vh;overflow: auto;">

    <ng-container *ngIf="type =='history'">
      <app-cs-user-followp-history [UserId]="UserId"></app-cs-user-followp-history>
    </ng-container>
    <ng-container *ngIf="type!='history'">
      <app-cs-user-add-follow-up [UserId]="UserId" [type]="type" [obj]="obj" (passEntry)="Event($event)"></app-cs-user-add-follow-up>
    
    </ng-container>
    
   
  </div>
  