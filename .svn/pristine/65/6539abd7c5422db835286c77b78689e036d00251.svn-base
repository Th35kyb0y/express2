.mobile-header {
    transition: all 0.5s;
    z-index: 997;
    height: 60px;
    box-shadow: 0px 2px 20px rgba(1, 41, 112, 0.1);
    background-color: #fff;
    padding-left: 20px;
    background-color: #c6000c;
}
.align-items-center {
    align-items: center !important;
}
.mobile-fixed-top {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    z-index: 1030;
}
.ms-auto {
    margin-left: auto !important;
}


.mobile-header .mobile-toggle-sidebar-btn {
    font-size: 32px;
    padding-left: 10px;
    cursor: pointer;
    color: #012970;
  }
  
  .mobile-header .search-bar {
    min-width: 360px;
    padding: 0 20px;
  }
  
  @media (max-width: 1199px) {
    .mobile-header .search-bar {
      position: fixed;
      top: 50px;
      left: 0;
      right: 0;
      padding: 20px;
      box-shadow: 0px 0px 15px 0px rgba(1, 41, 112, 0.1);
      background: white;
      z-index: 9999;
      transition: 0.3s;
      visibility: hidden;
      opacity: 0;
    }
  
    .mobile-header .search-bar-show {
      top: 60px;
      visibility: visible;
      opacity: 1;
    }
  }
  
  .mobile-header .search-form {
    width: 100%;
  }
  
  .mobile-header .search-form input {
    border: 0;
    font-size: 14px;
    color: #012970;
    border: 1px solid rgba(1, 41, 112, 0.2);
    padding: 7px 38px 7px 8px;
    border-radius: 3px;
    transition: 0.3s;
    width: 100%;
  }
  
  .mobile-header .search-form input:focus,
  .mobile-header .search-form input:hover {
    outline: none;
    box-shadow: 0 0 10px 0 rgba(1, 41, 112, 0.15);
    border: 1px solid rgba(1, 41, 112, 0.3);
  }
  
  .mobile-header .search-form button {
    border: 0;
    padding: 0;
    margin-left: -30px;
    background: none;
  }
  
  .mobile-header .search-form button i {
    color: #012970;
  }
  
  /*--------------------------------------------------------------
  # Header Nav
  --------------------------------------------------------------*/
  .mobile-header-nav ul {
    list-style: none;
  }
  
  .mobile-header-nav>ul {
    margin: 0;
    padding: 0;
  }
  
  .mobile-header-nav .nav-icon {
    font-size: 22px;
    color: #012970;
    margin-right: 25px;
    position: relative;
  }
  
  .mobile-header-nav .nav-profile {
    color: #012970;
  }
  
  .mobile-header-nav .nav-profile img {
    max-height: 36px;
  }
  
  .mobile-header-nav .nav-profile span {
    font-size: 14px;
    font-weight: 600;
  }
  
  .mobile-header-nav .badge-number {
    position: absolute;
    inset: -2px -5px auto auto;
    font-weight: normal;
    font-size: 12px;
    padding: 3px 6px;
  }
  
  .mobile-header-nav .notifications {
    inset: 8px -15px auto auto !important;
  }
  
  .mobile-header-nav .notifications .notification-item {
    display: flex;
    align-items: center;
    padding: 15px 10px;
    transition: 0.3s;
  }
  
  .mobile-header-nav .notifications .notification-item i {
    margin: 0 20px 0 10px;
    font-size: 24px;
  }
  
  .mobile-header-nav .notifications .notification-item h4 {
    font-size: 16px;
    font-weight: 600;
    margin-bottom: 5px;
  }
  
  .mobile-header-nav .notifications .notification-item p {
    font-size: 13px;
    margin-bottom: 3px;
    color: #919191;
  }
  
  .mobile-header-nav .notifications .notification-item:hover {
    background-color: #f6f9ff;
  }
  
  .mobile-header-nav .messages {
    inset: 8px -15px auto auto !important;
  }
  
  .mobile-header-nav .messages .message-item {
    padding: 15px 10px;
    transition: 0.3s;
  }
  
  .mobile-header-nav .messages .message-item a {
    display: flex;
  }
  
  .mobile-header-nav .messages .message-item img {
    margin: 0 20px 0 10px;
    max-height: 40px;
  }
  
  .mobile-header-nav .messages .message-item h4 {
    font-size: 16px;
    font-weight: 600;
    margin-bottom: 5px;
    color: #444444;
  }
  
  .mobile-header-nav .messages .message-item p {
    font-size: 13px;
    margin-bottom: 3px;
    color: #919191;
  }
  
  .mobile-header-nav .messages .message-item:hover {
    background-color: #f6f9ff;
  }
  
  .mobile-header-nav .profile {
    min-width: 240px;
    padding-bottom: 0;
    top: 8px !important;
  }
  
  .mobile-header-nav .profile .dropdown-header h6 {
    font-size: 18px;
    margin-bottom: 0;
    font-weight: 600;
    color: #444444;
  }
  
  .mobile-header-nav .profile .dropdown-header span {
    font-size: 14px;
  }
  
  .mobile-header-nav .profile .dropdown-item {
    font-size: 14px;
    padding: 10px 15px;
    transition: 0.3s;
  }
  
  .mobile-header-nav .profile .dropdown-item i {
    margin-right: 10px;
    font-size: 18px;
    line-height: 0;
  }
  
  .mobile-header-nav .profile .dropdown-item:hover {
    background-color: #f6f9ff;
  }
  

  
/*--------------------------------------------------------------
# Sidebar
--------------------------------------------------------------*/
.mobile-sidebar {
    position: fixed;
    top: 60px;
    left: 0;
    bottom: 0;
    width: 300px;
    z-index: 996;
    transition: all 0.3s;
    // padding: 20px;
    overflow-y: auto;
    scrollbar-width: thin;
    scrollbar-color: #aab7cf transparent;
    box-shadow: 0px 0px 20px rgba(1, 41, 112, 0.1);
    background-color: #fff;
  }
  
  @media (max-width: 1199px) {
    .mobile-sidebar {
      left: -300px;
    }
  }
  
  .mobile-sidebar::-webkit-scrollbar {
    width: 5px;
    height: 8px;
    background-color: #fff;
  }
  
  .mobile-sidebar::-webkit-scrollbar-thumb {
    background-color: #aab7cf;
  }
  
  @media (max-width: 700px) {
  
    // #main,
    // #footer {
    //   margin-left: 300px;
    // }
  }
  
  @media (max-width: 1199px) {
    .mobile-toggle-sidebar .mobile-sidebar {
      left: 0;
      width: 100%;
    }
  }
  
  @media (min-width: 900px) {
  
    .mobile-toggle-sidebar #main,
    .mobile-toggle-sidebar #footer {
      margin-left: 0;
    }
  
    .mobile-toggle-sidebar .mobile-sidebar {
      left: -300px;
    }
  }
  
  .mobile-sidebar-nav {
    padding: 0;
    margin: 0;
    list-style: none;
  }
  
  .mobile-sidebar-nav li {
    padding: 0;
    margin: 0;
    list-style: none;
  }
  
  .mobile-sidebar-nav .nav-item {
    margin-bottom: 5px;
  }
  
  .mobile-sidebar-nav .nav-heading {
    font-size: 11px;
    text-transform: uppercase;
    color: #899bbd;
    font-weight: 600;
    margin: 10px 0 5px 15px;
  }
  
  .mobile-sidebar-nav .nav-link {
    display: flex;
    align-items: center;
    font-size: 15px;
    font-weight: 600;
    color: #000;
    transition: 0.3;
    background: #f6f9ff;
    padding: 10px 15px;
    border-radius: 4px;
  }
  
  .mobile-sidebar-nav .nav-link i {
    font-size: 16px;
    margin-right: 10px;
    color: #000;
  }
  
  .mobile-sidebar-nav .nav-link.collapsed {
    color: #000;
  
  }
  
  .mobile-sidebar-nav .nav-link.collapsed i {
    color: #899bbd;
  }
  
  .mobile-sidebar-nav .nav-link:hover {
    color: #000;
    background: #f6f9ff;
  }
  
  .mobile-sidebar-nav .nav-link:hover i {
    color: #4154f1;
  }
  
  .mobile-sidebar-nav .nav-link .fa-caret-down{
    margin-right: 0;
    transition: transform 0.2s ease-in-out;
  }
  
  .mobile-sidebar-nav .nav-link:not(.collapsed) .fa-caret-down{
    transform: rotate(180deg);
  }
  
  .mobile-sidebar-nav .nav-content {
    padding: 5px 0 0 0;
    margin: 0;
    list-style: none;
  }
  
  .mobile-sidebar-nav .nav-content a {
    display: flex;
    align-items: center;
    font-size: 14px;
    font-weight: 600;
    color: #012970;
    transition: 0.3;
    padding: 10px 0 10px 40px;
    transition: 0.3s;
  }
  
  .mobile-sidebar-nav .nav-content a i {
    font-size: 6px;
    margin-right: 8px;
    line-height: 0;
    border-radius: 50%;
  }
  
  .mobile-sidebar-nav .nav-content a:hover,
  .mobile-sidebar-nav .nav-content a.active {
    color: #4154f1;
  }
  
  .mobile-sidebar-nav .nav-content a.active i {
    background-color: #4154f1;
  }
 
  
/*--------------------------------------------------------------
# Override some default Bootstrap stylings
--------------------------------------------------------------*/
/* Dropdown menus */
.mobile-dropdown-menu {
  border-radius: 4px;
  padding: 10px 0;
  animation-name: dropdown-animate;
  animation-duration: 0.2s;
  animation-fill-mode: both;
  border: 0;
  box-shadow: 0 5px 30px 0 rgba(82, 63, 105, 0.2);
}

.mobile-dropdown-menu .dropdown-header,
.mobile-dropdown-menu .dropdown-footer {
  text-align: center;
  font-size: 15px;
  padding: 10px 25px;
}

.mobile-dropdown-menu .dropdown-footer a {
  color: #444444;
  text-decoration: underline;
}

.mobile-dropdown-menu .dropdown-footer a:hover {
  text-decoration: none;
}

.mobile-dropdown-menu .dropdown-divider {
  color: #a5c5fe;
  margin: 0;
}

.mobile-dropdown-menu .dropdown-item {
  font-size: 14px;
  padding: 10px 15px;
  transition: 0.3s;
}

.mobile-dropdown-menu .dropdown-item i {
  margin-right: 10px;
  font-size: 18px;
  line-height: 0;
}

.mobile-dropdown-menu .dropdown-item:hover {
  background-color: #f6f9ff;
}

@media (min-width: 768px) {
  .mobile-dropdown-menu-arrow::before {
    content: "";
    width: 13px;
    height: 13px;
    background: #fff;
    position: absolute;
    top: -7px;
    right: 20px;
    transform: rotate(45deg);
    border-top: 1px solid #eaedf1;
    border-left: 1px solid #eaedf1;
  }
}

@keyframes dropdown-animate {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }

  0% {
    opacity: 0;
  }
}
.bg-color-m{
  background-color: #c6000c!important;
}